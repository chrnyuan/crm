<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
	<hibernate-mapping>
	
		<class name="com.zhiyou100.model.Notice" >
			
			<id name="notice_id" column="notice_id">
				<generator class="native"/>
			</id>
			<property name="receive_id" column="receive_id"></property>
			<property name="subject" column="subject"></property>
			<property name="text" column="text"></property>
			<property name="pub_time" column="pub_time"></property>
			<property name="expire_time" column="expire_time"></property>
			<property name="status" column="status"></property>
			<property name="remark" column="remark"></property>
			<property name="create_time" column="create_time"></property>
			<property name="creater" column="creater"></property>
			<property name="update_time" column="update_time"></property>
			<property name="updater" column="updater"></property>
		
		</class>

		
	</hibernate-mapping>    
